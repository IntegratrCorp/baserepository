{
    "name": "integratrcorp/baserepository",
    "description": "Base Repository Package",
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "ext-json": "*",
        "league/fractal": "^1.0.x-dev",
        "illuminate/support": "^10.x-dev"
    },
    "require-dev": {
        "orchestra/testbench": "8.0.x-dev",
        "phpunit/phpunit": "10.0.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Integratrcorp\\Baserepository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Integratrcorp\\Baserepository\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Angelo Zamora",
            "email": "a.zamora@integratrcorp.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Integratrcorp\\Baserepository\\ServiceProvider"
            ]
        }
    }
}
